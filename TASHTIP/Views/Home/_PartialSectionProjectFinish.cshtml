@model IEnumerable<TASHTIP.EF.Entities.Production.BussinessGallary>

<div class="row portfolio-container" data-aos="fade-up">
    @foreach (var item in Model)
    {
        <div class="col-lg-4 col-md-6 portfolio-item @item.Filter">
            <div class="portfolio-wrap">
                <img src="~/LayoutTASHTIP/Images/portfolio/@item.ProfileImage"
                     class="img-fluid"
                     alt="" />
                <div class="portfolio-info">
                    <h4>@item.ServicesName</h4>
                    <p>@item.ServicesName</p>
                    <div class="portfolio-links">
                        <a href="~/LayoutTASHTIP/Images/portfolio/@item.ProfileImage"
                           data-gallery="portfolioGallery"
                           class="portfolio-lightbox"
                           title="@item.ServicesName Unit">
                            <i class="bx bx-plus"></i>
                        </a>
                        <a asp-action="DetailsProperty" asp-controller="Home" asp-route-id="@item.ID" title="More Details">
                            <i class="bx bx-link"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    }


